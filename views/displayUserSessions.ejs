        <script src="angular/specificSession.js"></script>
    
    
    	<link rel="stylesheet" href="stylesheets/modern-business.css">
    	
        <link rel="stylesheet" href="stylesheets/style.css">


</head>
<body ng-app="sessions" ng-controller = "sessions" ng-init = "getSession(<%= sessionId %>)">

<% include navbar.ejs %>
 <div class="container">
        <!-- Intro Content -->
        <div class="row">
         <div class="col-lg-12">
                <h2 class="page-header">SESSION DETAILS: </h2>
                
            </div>
            <div class="col-md-6">
                <img class="img-responsive" src="http://placehold.it/750x450" alt="">
            </div>
             <div class="col-md-6">
                <h3>{{sessionName}} </h3><hr>
                <p>Speaker : {{speakerName}}</p>
                 <p>Timing : {{sessionTimings}}</p>
                  <p>Location : {{sessionLocation}}</p>
                <p>Description : {{sessionDesc}}</p>
             
                <hr>
                </div>
                <div class="row">
                <div class="col-md-6">
                <button id ="btn" ng-model="buttonSuccess" style="float:center;" ng-click="interested(<%= sessionId %>)" class="btn btn-default">Interested</button>  <p ng-model="attendees">  Attending = {{attendees}} </p><p ng-model="currentlyAttending">Curruntly Attending = {{currentlyAttending}}</p>
             </div></div>
             
        </div>
        </div>
        <!-- /.row -->
        </body>
